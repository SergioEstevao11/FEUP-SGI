    <transformations>
        <transformation id="origin_transform">
                <!-- <translate x="0" y="0" z="-0.5"/> -->
        </transformation>
    </transformations>

    <primitives>
        <primitive id="triangle">
            <triangle x1="0" x2="0" x3="1" y1="1" y2="0" y3="0" z1="0" z2="0" z3="0" />
        </primitive> 
        <primitive id="propulsor_triangle">
            <triangle x1="0.156" x2="0" x3="0.31" y1="0.99" y2="0" y3="0" z1="0" z2="0" z3="0" />
        </primitive> 
        <primitive id="triangle_wing_panel">
            <triangle x1="-1" y1="-1.7" z1="0" x2="1" y2="-1.7" z2="0" x3="0" y3="0" z3="0" />
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" slices="20" stacks="20" />
        </primitive>
        <primitive id="torus">
            <torus inner="0.3" outer="1" slices="40" loops="40" />
        </primitive>
        <primitive id="cylinder">      
            <cylinder base="1" top="1" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="cockpit_front">      
            <cylinder base="1" top="0.6" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="hexa_cylinder">      
            <cylinder base="1" top="1" height="1" slices="6" stacks="20" /> 
        </primitive>
        <primitive id="hexa_sphere">
            <sphere radius="1" slices="6" stacks="6" />
        </primitive>
        <primitive id="pipe_cockpit_cylinder">
            <cylinder base="0.6" top="0.3" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="pipe_panel_cylinder">
            <cylinder base="0.3" top="0.4" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="rectangle">
            <patch degree_u="1" parts_u="20" degree_v="1" parts_v="20" >
                <controlpoint x="0" y="0" z="0" />
                <controlpoint x="0" y="1" z="0" />
                <controlpoint x="1" y="0" z="0" />
                <controlpoint x="1" y="1" z="0" />  
            </patch>
        </primitive>
        <primitive id="tent">
            <patch degree_u="1" parts_u="20" degree_v="2" parts_v="20" >
                <controlpoint x="-1" y="-3" z="0" />
                <controlpoint x="-1" y="0" z="2" />
                <controlpoint x="-1" y="3" z="0" />
                <controlpoint x="1" y="-3" z="0" />
                <controlpoint x="1" y="0" z="2" />
                <controlpoint x="1" y="3" z="0" />  
            </patch>
        </primitive>
        <primitive id="tent_inverted">
               <patch degree_u="1" parts_u="20" degree_v="2" parts_v="20" >
                <controlpoint x="1" y="3" z="0" />
                <controlpoint x="1" y="0" z="-2" />
                <controlpoint x="1" y="-3" z="0" />
                <controlpoint x="-1" y="3" z="0" />
                <controlpoint x="-1" y="0" z="-2" />
                <controlpoint x="-1" y="-3" z="0" />  
            </patch>
        </primitive>
        <primitive id="half_barrel">
            <patch degree_u="3" parts_u="20" degree_v="3" parts_v="20" >
                <controlpoint x="-1.0" y="0" z="0" />
                <controlpoint x="-1.6666666666666665" y="0" z="0.41157974918903667" />
                <controlpoint x="-1.6666666666666665" y="0" z="1.5884202508109633" />
                <controlpoint x="-1.0" y="0" z="2.0" />
                <controlpoint x="-1.0" y="-1.3333333333333333" z="0" />
                <controlpoint x="-1.6666666666666665" y="-2.222222222222222" z="0.41157974918903667" />
                <controlpoint x="-1.6666666666666665" y="-2.222222222222222" z="1.5884202508109633" />
                <controlpoint x="-1.0" y="-1.3333333333333333" z="2.0" />
                <controlpoint x="1.0" y="-1.3333333333333333" z="0" />
                <controlpoint x="1.6666666666666665" y="-2.222222222222222" z="0.41157974918903667" />
                <controlpoint x="1.6666666666666665" y="-2.222222222222222" z="1.5884202508109633" />
                <controlpoint x="1.0" y="-1.3333333333333333" z="2.0" />
                <controlpoint x="1.0" y="0" z="0" />
                <controlpoint x="1.6666666666666665" y="0" z="0.41157974918903667" />
                <controlpoint x="1.6666666666666665" y="0" z="1.5884202508109633" />
                <controlpoint x="1.0" y="0" z="2.0" />
            </patch>
        </primitive>
        <primitive id="half_barrel_inverted">
            <patch degree_u="3" parts_u="20" degree_v="3" parts_v="20" >
               <controlpoint x="1.0" y="0" z="0" />
               <controlpoint x="1.6666666666666665" y="0" z="-0.41157974918903667" />
               <controlpoint x="1.6666666666666665" y="0" z="-1.5884202508109633" />
               <controlpoint x="1.0" y="0" z="-2.0" />
               <controlpoint x="1.0" y="1.3333333333333333" z="0" />
               <controlpoint x="1.6666666666666665" y="2.222222222222222" z="-0.41157974918903667" />
               <controlpoint x="1.6666666666666665" y="2.222222222222222" z="-1.5884202508109633" />
               <controlpoint x="1.0" y="1.3333333333333333" z="-2.0" />
               <controlpoint x="-1.0" y="1.3333333333333333" z="0" />
               <controlpoint x="-1.6666666666666665" y="2.222222222222222" z="-0.41157974918903667" />
               <controlpoint x="-1.6666666666666665" y="2.222222222222222" z="-1.5884202508109633" />
               <controlpoint x="-1.0" y="1.3333333333333333" z="-2.0" />
               <controlpoint x="-1.0" y="0" z="0" />
               <controlpoint x="-1.6666666666666665" y="0" z="-0.41157974918903667" />
               <controlpoint x="-1.6666666666666665" y="0" z="-1.5884202508109633" />
               <controlpoint x="-1.0" y="0" z="-2.0" />
            </patch>
        </primitive>
 
    </primitives>

    <animations>
    </animations>

    <components>
        <component id="demoRoot">
            <transformation>
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle"/>
                
            </children>
        </component>
    </components>
