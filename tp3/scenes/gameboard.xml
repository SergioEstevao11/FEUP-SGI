        <texture id ="mf_back" file="scenes/images/mf_back.png" />
        <texture id ="metal_tex" file="scenes/images/metal_tex.png" />
        <texture id ="grid" file="scenes/images/grid.png" />
        <texture id ="sp_tex" file="scenes/images/spaceship_tex.png" />
        <texture id ="cp_tex" file="scenes/images/cockpit_tex.jpg" />
        <texture id ="first_order" file="scenes/images/first_order.png" />
