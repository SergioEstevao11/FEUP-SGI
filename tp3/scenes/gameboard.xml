    <materials>
        <material id="demoMaterial" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0" g="0" b="0" a="1.0" />
            <specular r="0" g="0" b="0" a="1.0" />
        </material>
        <material id="light_grey_metal" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.01" g="0.01" b="0.01" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </material>
        <material id="propulsor_blue" shininess="10">
            <emission r="0.1" g="0.2" b="0.25" a="1.0" />
            <ambient r="0.1" g="0.3" b="0.6" a="1.0" />
            <diffuse r="0.2" g="0.7" b="0.9" a="1.0" />
            <specular r="0" g="0" b="0" a="1.0" />
        </material>

        <material id="night_black" shininess="7">
