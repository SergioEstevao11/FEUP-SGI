        <texture id="ring" file="scenes/images/ring.jpg" />
        <texture id="saturn" file="scenes/images/saturn.jpg" />
        <texture id="vent" file="scenes/images/vent.jpg" />
        <texture id="chess_board" file="scenes/images/chess_board.png" />
