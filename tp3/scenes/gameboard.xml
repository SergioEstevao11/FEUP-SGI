<sxs>
    <scene root="demoRoot" axis_length="40"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="13.6" y="34" z="57.8" />
            <to x="0" y="-2" z="0" />
        </perspective>

        <perspective id="Millenium Falcon POV" near="0.1" far="500" angle="45">
            <from x="3.5" y="1.5" z="5" />
            <to x="15" y="0" z="0" />
        </perspective>

        <perspective id="Front" near="0.1" far="500" angle="45">
            <from x="30" y="1.5" z="-1.5" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="Chaser Tie Fighter POV" near="0.1" far="500" angle="70">
            <from x="-16" y="5.5" z="-2.5" />
            <to x="0" y="-2" z="0" />
        </perspective>
        <perspective id="Saturn" near="0.1" far="500" angle="45">
            <from x="-25" y="5" z="-15" />
            <to x="15" y="15" z="15" />
        </perspective>
        <ortho id="Side Ortho" near="0.001" far="2000" left="-50" right="50" top="50" bottom="-50">
            <from x="0" y="0" z="20"/>
            <to x="0" y="-2" z="0"/>
            <up x="0" y="20" z="0"/><!-- optional, default 0,1,0 -->
        </ortho>
        <perspective id="Chase Camera" near="0.1" far="500" angle="45">
            <from x="31.25" y="6.25" z="6.25" />
            <to x="0" y="0" z="0" />
        </perspective>
        <ortho id="orthoUpView" near="0.01" far="500" left="-45" right="45" top="45" bottom="-45">
            <from x="0.00001" y="30" z="0"/>
            <to x="0" y="0" z="0"/>
            <up x="0" y="1" z="0"/>
            <!-- optional, default 0,1,0 -->
        </ortho>
    </views>

    <ambient>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </ambient>

    <lights>
        
        <omni id="main light" enabled="0">
            <location x="20" y="20" z="20" w="1.0" />
            <ambient r="1" g="1" b="1" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
            <attenuation constant="0.2" linear="0.0" quadratic="0.0"/>
        </omni>

        <spot id="falcon quadratic" enabled="0" angle="180" exponent="2">
            <location x="6" y="0" z="0" w="1.0" />
            <target x="10" y="0" z="0" w="1.0" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
            <attenuation constant="0.0" linear="0.0" quadratic="0.01" />
        </spot>
        <spot id="falcon constant" enabled="0" angle="180" exponent="2">
            <location x="6 " y="0" z="0" w="1.0" />
            <target x="10" y="0" z="0" w="1.0" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
            <attenuation constant="0.1" linear="0.0" quadratic="0.0" />
        </spot>
        <spot id="falcon linear" enabled="0" angle="180" exponent="2">
            <location x="6" y="0" z="0" w="1.0" />
            <target x="10" y="0" z="0" w="1.0" />
            <ambient r="1" g="1" b="1" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
            <attenuation constant="0.0" linear="0.1" quadratic="0.0" />
        </spot>
        <spot id="tf 1" enabled="0" angle="180" exponent="1">
            <location x="13.5" y="-2" z="0.75" w="1.0" />
            <target x="0" y="0" z="0" w="1.0" />
            <ambient r="1" g="1" b="1" a="1.0" />
            <diffuse r="1" g="0" b="0" a="1.0" />
            <specular r="0" g="0" b="0" a="1.0" />
            <attenuation constant="0.0" linear="0.1" quadratic="0.0" />
        </spot>
        <spot id="tf 2" enabled="0" angle="180" exponent="2">
            <location x="-14" y="5" z="-2" w="1.0" />
            <target x="0" y="0" z="0" w="1.0" />
            <ambient r="1" g="1" b="1" a="1.0" />
            <diffuse r="0" g="1" b="0" a="1.0" />
            <specular r="0" g="0" b="0" a="1.0" />
            <attenuation constant="0.0" linear="0.2" quadratic="0.0" />
        </spot>
        <spot id="tf 3" enabled="0" angle="180" exponent="2">
            <location x="-14" y="4" z="9" w="1.0" />
            <target x="0" y="0" z="0" w="1.0" />
            <ambient r="1" g="1" b="1" a="1.0" />
            <diffuse r="0" g="0" b="1" a="1.0" />
            <specular r="0" g="0" b="0" a="1.0" />
            <attenuation constant="0.0" linear="0.0" quadratic="0.1" />
        </spot>
        <omni id="sun light" enabled="1">
            <location x="-20" y="10" z="10" w="1.0" />
            <ambient r="1" g="1" b="1" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
            <attenuation constant="0.0" linear="0.01" quadratic="0.0"/>
        </omni>
    </lights>

    <textures>
        <texture id="demoTexture" file="scenes/images/vidral.jpg" />
        <texture id="solar_panel" file="scenes/images/solar_panel.jpg" />
        <texture id="sky" file="scenes/images/space/universe.jpg" />
        <texture id="sun_texture" file="scenes/images/space/sun.jpg" />
        <texture id ="light_ray" file="scenes/images/light_ray.png" />
        <texture id ="mf_front_rec" file="scenes/images/mf_front_rec.png" />
        <texture id ="mf_middle_big_square" file="scenes/images/mf_middle_big_square.png" />
        <texture id ="mf_middle_square" file="scenes/images/mf_middle_square.png" />
        <texture id ="mf_side_front_rec" file="scenes/images/mf_side_front_rec.png" />
