        <material id="checkers_black" shininess="7">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.01" g="0.01" b="0.01" a="1.0" />
            <diffuse r="0.05" g="0.05" b="0.05" a="1.0" />
