    <transformations>
        <transformation id="origin_transform">
                <!-- <translate x="0" y="0" z="-0.5"/> -->
        </transformation>
    </transformations>

    <primitives>
        <primitive id="triangle">
            <triangle x1="0" x2="0" x3="1" y1="1" y2="0" y3="0" z1="0" z2="0" z3="0" />
        </primitive> 
        <primitive id="propulsor_triangle">
            <triangle x1="0.156" x2="0" x3="0.31" y1="0.99" y2="0" y3="0" z1="0" z2="0" z3="0" />
        </primitive> 
        <primitive id="triangle_wing_panel">
            <triangle x1="-1" y1="-1.7" z1="0" x2="1" y2="-1.7" z2="0" x3="0" y3="0" z3="0" />
        </primitive>
        <!-- <primitive id="rectangle">
            <rectangle x1="0" x2="1" y1="0" y2="1" />
        </primitive> -->
        <primitive id="sphere">
            <sphere radius="1" slices="20" stacks="20" />
        </primitive>
        <primitive id="torus">
            <torus inner="0.3" outer="1" slices="40" loops="40" />
        </primitive>
        <primitive id="cylinder">      
            <cylinder base="1" top="1" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="cockpit_front">      
            <cylinder base="1" top="0.6" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="hexa_cylinder">      
            <cylinder base="1" top="1" height="1" slices="6" stacks="20" /> 
        </primitive>
        <primitive id="hexa_sphere">
            <sphere radius="1" slices="6" stacks="6" />
        </primitive>
        <primitive id="pipe_cockpit_cylinder">
            <cylinder base="0.6" top="0.3" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="pipe_panel_cylinder">
            <cylinder base="0.3" top="0.4" height="1" slices="20" stacks="20" /> 
        </primitive>
        <primitive id="rectangle">
            <patch degree_u="1" parts_u="20" degree_v="1" parts_v="20" >
                <controlpoint x="0" y="0" z="0" />
                <controlpoint x="0" y="1" z="0" />
                <controlpoint x="1" y="0" z="0" />
                <controlpoint x="1" y="1" z="0" />  
            </patch>
        </primitive>
        <primitive id="tent">
            <patch degree_u="1" parts_u="20" degree_v="2" parts_v="20" >
                <controlpoint x="-1" y="-3" z="0" />
                <controlpoint x="-1" y="0" z="2" />
                <controlpoint x="-1" y="3" z="0" />
                <controlpoint x="1" y="-3" z="0" />
                <controlpoint x="1" y="0" z="2" />
                <controlpoint x="1" y="3" z="0" />  
            </patch>
        </primitive>
        <primitive id="tent_inverted">
               <patch degree_u="1" parts_u="20" degree_v="2" parts_v="20" >
                <controlpoint x="1" y="3" z="0" />
                <controlpoint x="1" y="0" z="-2" />
                <controlpoint x="1" y="-3" z="0" />
                <controlpoint x="-1" y="3" z="0" />
                <controlpoint x="-1" y="0" z="-2" />
                <controlpoint x="-1" y="-3" z="0" />  
            </patch>
        </primitive>
        <primitive id="half_barrel">
            <patch degree_u="3" parts_u="20" degree_v="3" parts_v="20" >
                <controlpoint x="-1.0" y="0" z="0" />
                <controlpoint x="-1.6666666666666665" y="0" z="0.41157974918903667" />
                <controlpoint x="-1.6666666666666665" y="0" z="1.5884202508109633" />
                <controlpoint x="-1.0" y="0" z="2.0" />
                <controlpoint x="-1.0" y="-1.3333333333333333" z="0" />
                <controlpoint x="-1.6666666666666665" y="-2.222222222222222" z="0.41157974918903667" />
                <controlpoint x="-1.6666666666666665" y="-2.222222222222222" z="1.5884202508109633" />
                <controlpoint x="-1.0" y="-1.3333333333333333" z="2.0" />
                <controlpoint x="1.0" y="-1.3333333333333333" z="0" />
                <controlpoint x="1.6666666666666665" y="-2.222222222222222" z="0.41157974918903667" />
                <controlpoint x="1.6666666666666665" y="-2.222222222222222" z="1.5884202508109633" />
                <controlpoint x="1.0" y="-1.3333333333333333" z="2.0" />
                <controlpoint x="1.0" y="0" z="0" />
                <controlpoint x="1.6666666666666665" y="0" z="0.41157974918903667" />
                <controlpoint x="1.6666666666666665" y="0" z="1.5884202508109633" />
                <controlpoint x="1.0" y="0" z="2.0" />
            </patch>
        </primitive>
        <primitive id="half_barrel_inverted">
            <patch degree_u="3" parts_u="20" degree_v="3" parts_v="20" >
               <controlpoint x="1.0" y="0" z="0" />
               <controlpoint x="1.6666666666666665" y="0" z="-0.41157974918903667" />
               <controlpoint x="1.6666666666666665" y="0" z="-1.5884202508109633" />
               <controlpoint x="1.0" y="0" z="-2.0" />
               <controlpoint x="1.0" y="1.3333333333333333" z="0" />
               <controlpoint x="1.6666666666666665" y="2.222222222222222" z="-0.41157974918903667" />
               <controlpoint x="1.6666666666666665" y="2.222222222222222" z="-1.5884202508109633" />
               <controlpoint x="1.0" y="1.3333333333333333" z="-2.0" />
               <controlpoint x="-1.0" y="1.3333333333333333" z="0" />
               <controlpoint x="-1.6666666666666665" y="2.222222222222222" z="-0.41157974918903667" />
               <controlpoint x="-1.6666666666666665" y="2.222222222222222" z="-1.5884202508109633" />
               <controlpoint x="-1.0" y="1.3333333333333333" z="-2.0" />
               <controlpoint x="-1.0" y="0" z="0" />
               <controlpoint x="-1.6666666666666665" y="0" z="-0.41157974918903667" />
               <controlpoint x="-1.6666666666666665" y="0" z="-1.5884202508109633" />
               <controlpoint x="-1.0" y="0" z="-2.0" />
            </patch>
        </primitive>
 
    </primitives>

     <animations>
     </animations>

    <components>

        <component id="lateralZPos">
            <transformation>
                <translate x="0" y="0" z="-0.5"/>
                <rotate axis="x" angle="90" />
                <scale x="1" y="0.5" z="1" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="metal_tex" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" /> 
            </children>
        </component>

        <component id="lateralZNeg">
            <transformation>
                <rotate axis="x" angle="180" />
                <translate x="0" y="-1" z="0.5"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="lateralZPos" /> 
            </children>
        </component>

        <component id="lateralsZ">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="lateralZPos" /> 
                <componentref id="lateralZNeg" /> 
            </children>
        </component>

        <component id="lateralsX">
            <transformation>
                <translate x="1" y="0" z="0"/>
                <rotate axis="z" angle="90" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="lateralsZ" /> 
            </children>
        </component>

        <component id="top">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="chess_board" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" /> 
            </children>
        </component>



        <component id="base">
            <transformation>
                <rotate axis="x" angle="180" />

                <translate x="0" y="-1" z="0.5"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="metal_tex" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="rectangle" /> 
            </children>
        </component>

        <component id="board">
            <transformation>
                <rotate axis="x" angle="-90" />
                <scale x="8" y="8" z="1.5" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="lateralsX" /> 
                <componentref id="lateralsZ" /> 
                <componentref id="base" />
                <componentref id="top" />
            </children>
        </component>

        <component id="checker_piece_cover">
            <transformation>
                <scale x="1" y="1" z="0.01"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="sphere"/>
            </children>
        </component>
        <component id="checker_piece_cover1">
            <transformation>
                <translate x="0" y="0" z="1"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="checker_piece_cover"/>
            </children>
            <highlighted r="0.6" g="0.1" b="0.1" scale_h="1.2"/>

        </component>
        <component id="checker_piece">
            <transformation>
                <translate x="0.5" y="0.15" z="-0.5"/>
                <rotate axis="x" angle="90"/>
                <scale x="0.3" y="0.3" z="0.15"/>
            </transformation>
            <materials>
                <material id="checkers_black" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="checker_piece_cover"/>
                <componentref id="checker_piece_cover1"/>
                <primitiveref id="cylinder"/>
            </children>
        </component>

        <component id="demoRoot">
            <transformation>
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture" length_s="1" length_t="1"/>
            <children>
                <componentref id="board" />
                <componentref id="checker_piece" /> 
            </children>
        </component>

    </components>
</sxs>